.card.text-center
  .card-header
    %ul.nav.nav-pills.card-header-pills
      %li.nav-item
        %a.nav-link.active{href: '${2:#}'} ${1:Active pill}
      %li.nav-item
        %a.nav-link{href: '${4:#}'} ${3:Pill}
      %li.nav-item
        %a.nav-link.disabled{href: '${6:#}'} ${5:Disabled pill}
  .card-body
    %h4.card-title ${7:Title}
    %p.card-text ${8:Body}
