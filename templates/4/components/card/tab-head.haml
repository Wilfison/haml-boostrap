.card.text-center
  .card-header
    %ul.nav.nav-tabs.card-header-tabs
      %li.nav-item
        %a.nav-link.active{href: '${2:#}'} ${1:Active tab}
      %li.nav-item
        %a.nav-link{href: '${4:#}'} ${3:Tab}
      %li.nav-item
        %a.nav-link.disabled{href: '${6:#}'} ${5:Disabled tab}
  .card-body
    %h4.card-title ${7:Title}
    %p.card-text ${8:Body}
