%ul.nav{class: "nav-${1:tabs|pills}"}
  %li.nav-item
    %a.nav-link.active{href: "${3:#}"} ${2:Active}
  %li.nav-item.dropdown
    %a.nav-link.dropdown-toggle{"aria-expanded": "false", "aria-haspopup": "true", "data-toggle": "dropdown", href: "#", role: "button"} ${4:Dropdown}
    .dropdown-menu
      %a.dropdown-item{href: "${6:#}"} ${5:Action}
      %a.dropdown-item{href: "${8:#}"} ${7:Another action}
      .dropdown-divider
      %a.dropdown-item{href: "${10:#}"} ${9:Action}
  %li.nav-item
    %a.nav-link{href: "${12:#}"} ${11:Another link}
  %li.nav-item
    %a.nav-link.disabled{href: "#"} ${13:Disabled}
