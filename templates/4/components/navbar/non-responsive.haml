%nav.navbar.navbar-expand.navbar-light.bg-faded
  %a.navbar-brand{href: '${2:#}'} ${1:Navbar}
  %ul.navbar-nav.mr-auto.mt-2.mt-lg-0
    %li.nav-item.active
      %a.nav-link{href: '${4:#}'}
        ${3:Active}
        %span.sr-only (current)
    %li.nav-item
      %a.nav-link{href: '${6:#}'} ${5:Link}
    %li.nav-item.dropdown
      %a.nav-link.dropdown-toggle{'aria-expanded': 'false', 'aria-haspopup': 'true', 'data-toggle': 'dropdown', href: '${8:#}', id: '${9:dropdownId}'} ${7:Dropdown}
      .dropdown-menu{'aria-labelledby': '${9:dropdownId}'}
        %a.dropdown-item{href: '${11:#}'} ${10:Action 1}
        %a.dropdown-item{href: '${13:#}'} ${12:Action 2}
  %form.form-inline.my-2.my-lg-0
    %input.form-control.mr-sm-2{placeholder: '${17:Search}', type: 'text'}/
    %button.btn.btn-outline-success.my-2.my-sm-0{type: 'submit'} ${18:Search}
